<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.input.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="530.0" prefWidth="730.0" stylesheets="@DarkTheme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="asa.address.view.RootLayoutController">
<top><MenuBar BorderPane.alignment="CENTER">
  <menus>
    <Menu mnemonicParsing="false" text="File">
      <items><MenuItem mnemonicParsing="false" onAction="#handleNew" text="New">
<accelerator>
<KeyCodeCombination alt="UP" code="N" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
</accelerator></MenuItem><MenuItem mnemonicParsing="false" onAction="#handleOpen" text="Open...">
<accelerator>
<KeyCodeCombination alt="UP" code="O" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
</accelerator></MenuItem>
        <MenuItem mnemonicParsing="false" onAction="#handleSave" text="Save">
<accelerator>
<KeyCodeCombination alt="UP" code="S" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
</accelerator></MenuItem>
                  <MenuItem mnemonicParsing="false" onAction="#handleSaveXLS" text="Save XLS" />
        <MenuItem mnemonicParsing="false" onAction="#handleSaveAs" text="Save As..." /><MenuItem mnemonicParsing="false" onAction="#handleExit" text="Exit" />
      </items>
    </Menu>
            <Menu mnemonicParsing="false" text="Email">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#handleSingleMail" text="Send a single mail" />
                  <MenuItem mnemonicParsing="false" onAction="#handleGroupMail" text="Send a group mail" />
                  <MenuItem mnemonicParsing="false" onAction="#handleRemainder" text="Send remainder mails" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Duty Roster">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#handleChange" text="Change altar service" />
                  <MenuItem mnemonicParsing="false" onAction="#handleCreate" text="Schedule masses" />
              </items>
            </Menu>
    <Menu mnemonicParsing="false" text="Statistics">
      <items>
        <MenuItem mnemonicParsing="false" onAction="#handleShowBirthdayStatistics" text="Show Statistics" />
      </items>
    </Menu>
    <Menu mnemonicParsing="false" text="Help">
      <items>
        <MenuItem mnemonicParsing="false" onAction="#handleAbout" text="About" />
      </items>
    </Menu>
  </menus>
</MenuBar>
</top></BorderPane>
