<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="asa.address.mail.GroupMailController">
   <children>
      <GridPane layoutX="150.0" layoutY="116.0" prefHeight="500.0" prefWidth="500.0" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
        <columnConstraints>
          <ColumnConstraints hgrow="NEVER" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="sender" />
            <Label text="password" GridPane.rowIndex="1" />
            <Label text="subject" GridPane.rowIndex="2" />
            <Label text="message" GridPane.rowIndex="3" />
            <Label text="attachment?" GridPane.rowIndex="5" />
            <CheckBox fx:id="cbAttachment" mnemonicParsing="false" text="0 - 3" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <TextField fx:id="senderLabel" GridPane.columnIndex="1" />
            <TextField fx:id="subjectLabel" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextArea fx:id="textLabel" prefHeight="151.0" prefWidth="250.0" GridPane.columnIndex="1" GridPane.rowIndex="3" GridPane.rowSpan="2" />
            <PasswordField fx:id="passwordLabel" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button layoutX="402.0" layoutY="460.0" mnemonicParsing="false" onAction="#handleContinue" text="continue..." AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" />
   </children>
</AnchorPane>
